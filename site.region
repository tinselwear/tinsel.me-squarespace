<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang=""> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" type="text/css" href="//cloud.typography.com/7304974/716028/css/fonts.css" />
    <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
    {squarespace-headers}
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">

    <div class="container">

      <header id="header" class="header">
        <!--Site title or logo.-->
        {.section website}

          <h1 class="site-title" data-content-field="site-title">
            <a href="/">
              {.section logoImageUrl}
               <img class="logo" src="{logoImageUrl}?format=original" alt="{siteTitle}" />
              {.or}
                {siteTitle}
              {.end}
            </a>
          </h1>
          
          <!--Site tagline.-->
          {.section siteTagLine}
            <p class="site-tagline">{@}</p>
          {.end}

        {.end}

      </header>

    <!--Main navigation.
    <squarespace:navigation navigationId="mainNav" template="navigation" />
    -->

    <nav class="navbar navbar-inverse navbar-fixed-top header-nav" role="navigation">
      <div class="container-fluid">

        <div class="navbar-header">
          <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <a href="/" class="home-link pull-left"><img src="/assets/header-logo-m.png"></a>
          <a href="/" class="navbar-brand sr-only" >Tinsel</a>
        </div>

        <div id="navbar" class="navbar-collapse collapse">
          <button class="btn btn-primary navbar-btn btn-cta">I WANT ONE</button>
          <ul class="nav navbar-nav navbar-right">
              <li><a href="/blog">blog</a></li>
              <li><a href="/team">team</a></li>
              <li role="presentation" class="instagram">
                <a href="instagram"><img src="/assets/instagram-icon.png" alt="instagram"></a></li>
              <li role="presentation" class="pinterest">
                <a href="pinterest"><img src="/assets/pinterest-icon.png" alt="pinterest"></a>
              </li>
              <li role="presentation" class="twitter">
                <a href="twitter"><img src="/assets/twitter-icon.png" alt="twitter"></a>
              </li>
              <li role="presentation" class="facebook">
                <a href="facebook"><img src="/assets/facebook-icon.png" alt="facebook"></a>
              </li>
              <li role="presentation" class="email">
                <a href="email"><img src="/assets/email-icon.png" alt="mailto: hello@tinsel.me"></a>
              </li>
          </ul>
        </div><!--/.navbar-collapse -->
    
      </div>
    </nav>

    <div class="container-fluid">

      <!--Content injection point.-->
      <main id="content" class="content" role="main" data-content-field="main-content">
       {squarespace.main-content}
      </main>

    </div> <!-- /container -->

    <footer class="clearfix">
      <!--Footer with open block field.
      <squarespace:block-field id="footerBlocks" columns="12" />
      -->
      <div class="row content">
        <a href="/" class="footer-brand">
          <img alt="Tinsel" src="/assets/footer-logo.png">
        </a>
        <div class="col-sm-6">
          <ul class="nav nav-pills page-links">
            <li role="presentation" ><a href="/blog">blog</a></li>
            <li role="presentation" ><a href="/team">team</a></li>
          </ul>
        </div>
        <div class="col-sm-6">
          <ul class="nav nav-pills social-links">
            <li role="presentation" class="instagram"><a href="instagram"><img src="/assets/instagram-icon.png"></a></li>
            <li role="presentation" class="pinterest"><a href="pinterest"><img src="/assets/pinterest-icon.png"></a></li>
            <li role="presentation" class="twitter"><a href="twitter"><img src="/assets/twitter-icon.png"></a></li>
            <li role="presentation" class="facebook"><a href="facebook"><img src="/assets/facebook-icon.png"></a></li>
            <li role="presentation" class="email"><a href="email"><img src="/assets/email-icon.png"></a></li>
          </ul>
        </div>
      </div>
      <p class="sr-only">&copy; Tinsel 2014</p>
    </footer>

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="/assets/vendor/jquery-1.11.2.min.js"><\/script>')</script>
    <!--Combo and minify the scripts when you're not logged in.-->
    <squarespace:script src="vendor/bootstrap.min.js" />
    <squarespace:script src="plugins.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <squarespace:script src="site.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />
    <squarespace:script src="main.js" combo="{.if authenticatedAccount}false{.or}true{.end}" />

    <!--Injection point for tracking scripts and user content from the code injection tab.-->
    {squarespace-footers}
    
  </body>
</html> 
