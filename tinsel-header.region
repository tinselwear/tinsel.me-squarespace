<!doctype html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang=""> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8" lang=""> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9" lang=""> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang=""> <!--<![endif]-->
  <head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <link rel="stylesheet" type="text/css" href="//cloud.typography.com/7304974/716028/css/fonts.css" />
    <!--Include sqs scripts, meta tags, and user content from the code injection tab.-->
    {squarespace-headers}

    <link rel="stylesheet" type="text/css" href="//{website.identifier}.squarespace.com/assets/styles/tinsel-fonts.css" />
    <link rel="stylesheet" type="text/css" href="//{website.identifier}.squarespace.com/assets/styles/bootstrap.min.css" />
    <link rel="stylesheet" type="text/css" href="//{website.identifier}.squarespace.com/assets/styles/tinsel-bootstrap-theme.css" />
    <link rel="stylesheet" type="text/css" href="//{website.identifier}.squarespace.com/assets/styles/main.css" />

    <script src="//ajax.googleapis.com/ajax/libs/jquery/1.11.2/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="/assets/vendor/jquery-1.11.2.min.js"><\/script>')</script>
  </head>
  <body id="{squarespace.page-id}" class="{squarespace.page-classes}">
    <script>
      /*
          Removing the standard styles so only 
          the custom home page styles apply.
          This is super hacky. Never do this.
      */
      //;(function() {$(document.head).children('link[href*="/site.css"]:first').remove();})();
    </script>

    <!--Main navigation.
    <squarespace:navigation navigationId="mainNav" template="navigation" />
    -->

    <header>
      <nav class="navbar navbar-inverse navbar-fixed-top affix-top header-nav" data-spy="affix" data-offset-top="60" role="navigation">
        <div class="container-fluid">

          <div class="navbar-header">
          {.section website}
            <a href="/" class="home-link pull-left">
              {.section logoImageUrl}
               <img class="logo" src="{logoImageUrl}?format=original" alt="{siteTitle}" />
              {.or}
               <img class="logo" src="/assets/header-logo.png" alt="{siteTitle}" />
               <span class="sr-only">{siteTitle}</span>
              {.end}
            </a>
            {.section siteTagLine}
              <p class="site-tagline sr-only">{@}</p>
            {.end}

          {.end}
          </div>


            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
 
          <button class="btn btn-primary navbar-btn btn-cta">I WANT ONE</button>


          <div class="navbar-form ss-block">
            <squarespace:block-field id="headerBlock" label="Header Block" columns="1" />
          </div>

          <div class="navbar-collapse collapse">
            <ul class="nav nav-pills navbar-nav page-links">
                <li><a href="/blog">blog</a></li>
                <!-- <li><a href="/team">team</a></li> -->
            </ul>
          </div><!--/.navbar-collapse -->
        </div>
      </nav>
    </header>
